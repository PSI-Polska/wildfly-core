<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">

    <!-- This is a false positive because the core itself does not use json-smart or accessors-smart. Those libraries are used by mockserver-netty in test (wildfly-core-testsuite-share) module. -->
    <suppress>
        <notes><![CDATA[
   file name: json-smart-2.3.jar
   file name: accessors-smart-1.2.jar
   ]]></notes>
        <packageUrl regex="true">^pkg:maven/net\.minidev/(json-smart|accessors\-smart).*$</packageUrl>
        <cve>CVE-2021-27568</cve>
        <cve>CVE-2021-31684</cve>
        <cve>CVE-2023-1370</cve>
    </suppress>

    <!-- This is a false positive because wildfly-core itself does not use mockserver libraries. Mockserver is used only for test purposes (wildfly-core-testsuite-shared module). -->
    <suppress>
        <notes><![CDATA[
   file name: mockserver-core-5.8.1.jar
   ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.mock\-server/mockserver\-.*$</packageUrl>
        <cve>CVE-2021-32827</cve>
    </suppress>

    <!-- [wildfly-core] Applies to versions prior to wildfly 21.0.0 -->
    <suppress>
        <notes><![CDATA[
   file name: wildfly-legacy-versions-6.0.1.Final.jar
   file name: wildfly-legacy-spi-6.0.1.Final.jar
   ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.wildfly\.legacy\.test/wildfly\-legacy\-.*$</packageUrl>
        <cve>CVE-2020-10718</cve>
        <cve>CVE-2020-10740</cve>
    </suppress>

    <!-- CVE-2022-1259 is directly related to undertow-core -->
    <suppress>
        <notes><![CDATA[
       file name: undertow-server-1.10.1.Final.jar
       ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.wildfly\.security\.elytron\-web/undertow\-server@.*$</packageUrl>
        <vulnerabilityName>CVE-2022-1259</vulnerabilityName>
    </suppress>

    <!-- False positive, this vulnerability affects module used in wildfly tests only -->
    <suppress>
        <notes><![CDATA[
		file name: commons-text-1.8.jar
		]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.apache\.commons/commons\-text@.*$</packageUrl>
        <vulnerabilityName>CVE-2022-42889</vulnerabilityName>
    </suppress>

    <suppress>
        <notes><![CDATA[
        file name: wildfly-legacy-spi-7.0.0.Final.jar
        file name: wildfly-legacy-versions-7.0.0.Final.jar
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.wildfly\.legacy\.test/wildfly\-legacy\-.*@.*$</packageUrl>
        <cve>CVE-2022-1278</cve>
    </suppress>
    <suppress>
        <notes><![CDATA[
        file name: plexus-interpolation-1.24.jar
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.codehaus\.plexus/plexus\-interpolation@.*$</packageUrl>
        <cve>CVE-2022-4244</cve>
    </suppress>
    <suppress>
        <notes><![CDATA[
        file name: plexus-component-annotations-1.7.1.jar
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.codehaus\.plexus/plexus\-component\-annotations@.*$</packageUrl>
        <cve>CVE-2022-4244</cve>
    </suppress>

    <!-- Dependency used only in test modules -->
    <suppress>
        <notes><![CDATA[
        file name: json-path-2.4.0.jar
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.jayway\.jsonpath/json\-path@.*$</packageUrl>
        <vulnerabilityName>CVE-2023-51074</vulnerabilityName>
    </suppress>
</suppressions>
